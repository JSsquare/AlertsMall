<h1>Hit Count</h1>
<h2>Unique User # <%= @unique_impression_count%></h2>


<div id="div-for-tweetlist" >
  <%= will_paginate @impressions  %>
  <% count = ((params[:page] || 1).to_i - 1) * 10  %>


  <table class="table table-hover">
    <tr>
      <th>SL #</th>
      <th>IP</th>
      <th>IP Description</th>
      <th>Date | Time</th>
    </tr>

    <% @impressions.each do |impression| %>
            <tr>
              <td><%= count %></td>
              <td><%= impression.ip_address %></td>
              <td><%= Whois.whois(impression.ip_address).parser.parsers.to_s rescue "Info Unavailable ATM" %></td>
              <td><%= impression.created_at.strftime("%m/%d/%Y %I:%M%p")%></td>
            </tr>
        <% count += 1 %>
    <% end %>
  </table>

</div>


<script type="text/javascript">
    $(document).ready(function(){

        $("a[class=tooltip]").hover(
                function () {
                    var position = $(this).position();
                    $(this).children("div.content").addClass('tip').css('left',position.left);
                },
                function () {
                    $(this).children("div.content").removeClass('tip');
                }
        );
    });

</script>